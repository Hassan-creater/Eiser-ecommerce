<!doctype html>
<html>
<head>
  <title>{{page_title}}</title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="description" content="{{ page_description | escape }}">
  <link rel="canonical" href="{{ canonical_url }}">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.7.2/css/all.min.css" integrity="sha512-Evv84Mr4kqVGRNSgIGL/F/aIDqQb7xQ2vcrdIwxfjThSH8CSR7PBEakCr51Ck+w+/U6swU2Im1vVX0SVk9ABhg==" crossorigin="anonymous" referrerpolicy="no-referrer" />
  <link rel="shortcut icon" href="{{ 'favicon.ico' |  asset_url }}" type="image/x-icon">
  <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
  {{ content_for_header }} 

  
  <!-- Header hook for plugins -->
  {{ 'application.css' | asset_url | stylesheet_tag }}
  {{ 'custom.css' | asset_url | stylesheet_tag }}
  
  <script src="{{ 'application.js' | asset_url }}" defer></script>
  <script src="https://cdn.jsdelivr.net/gh/alpinejs/alpine@v2.8.2/dist/alpine.min.js" defer></script>

  <style>

    
    [x-cloak] { display: none !important; }
    
   

  


  </style>
</head>
<body>

 {% unless template contains 'customers/login' or template contains 'customers/register' %}
  {% section 'header' %}
 {% endunless %}

  <main class="overflow-x-hidden" role="main">
    {{ content_for_layout }}
    
   
  </main>


  {% unless template contains 'customers/login' or template contains 'customers/register' or template contains 'customers/account' %}
    {% section 'footer' %}
    
   {% endunless %}
  
  
</body>
</html>
